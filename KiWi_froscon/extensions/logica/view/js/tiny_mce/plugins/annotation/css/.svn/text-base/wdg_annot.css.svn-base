.annotpane{
				 border: solid 1px #CCC;
				 width:150px;
				 heigth:100%;
				 font-size: 0.95em;
				 }
				 .annotcontent {
				 }
				 .annot-bar-header{
				 width: 100%;
				 height: 54px;
				 font-size: 1.4em;
				 text-align:center;
				 border-bottom: solid 1px #CCC;
				 background:#F0F0EE;
				 }
				 .annot-subitems {
				 text-align: justify;
				 margin-left:7px;
				 }
				 .no-annots{
				 font-style: italic;
