				 .annotcontent a img{
				 border: none;
				 }
				 .annotcontent .annot-cnt{
				 font-size : 0.9em;
				 }
				 .annotcontent img.actbutton{
				 vertical-align: bottom;
				 }

a.annot-sel-link:link, a.annot-sel-link:visited, a.annot-sel-link:hover{
	color:black;
