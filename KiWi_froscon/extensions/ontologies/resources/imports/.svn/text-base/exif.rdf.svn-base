<rdf:RDF
  xmlns:rdfs="&rdfs;"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:foaf="&foaf;"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:exif="&exif;#"
  xmlns:ex="http://purl.org/net/ns/ex#"
  xmlns:ont="http://purl.org/net/ns/ontology-annot#"
  xml:base="&exif;"
