         <context-root>/KiWiIdpServlet</context-root>
      </web>
   </module>-->

   <module>
      <ejb>KiWi.jar</ejb>
   </module>

   <!--  Quartz Dispatcher -->
   <module>
      <ejb>quartz-1.6.5.jar</ejb>
   </module>

   <!-- Seam and EL -->
   <module>
       <ejb>jboss-seam.jar</ejb>
   </module>

   <!--  Wiki extension -->
   <module>
       <ejb>kiwiext-wiki.jar</ejb>
   </module>

   <!-- Admin extension -->
   <module>
       <ejb>kiwiext-admin.jar</ejb>
   </module>

   <!-- Inspector extension -->
   <module>
       <ejb>kiwiext-inspector.jar</ejb>
