<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="dashboard">
	<entry-point class='kiwi.view.dashboard.main.client.Dashboard'/>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.user.History"/>
    
  	<inherits name="com.smartgwt.SmartGwt"/>
  	<inherits name="kiwi.transport.TransportObjects"/>

  	<inherits name="kiwi.view.dashboard.profile.Profile"/>
  	<inherits name="kiwi.view.dashboard.profilephoto.ProfilePhoto"/>
  	<inherits name="kiwi.view.dashboard.dummy.Dummy"/>

<!-- 
    <inherits name="com.smartgwt.SmartGwtNoScript"/>
    <inherits name="com.smartgwt.tools.SmartGwtTools"/>
    <set-property name="user.agent" value="gecko1_8,gecko,safari"/>
 -->
    <set-property name="user.agent" value="gecko"/>


    <!-- english language, independent of country -->
    <extend-property name="locale" values="en"/>
  
</module>
