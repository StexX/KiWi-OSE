.cluster {
	width:500px;
	height:300px;
	position:absolute;
}

.clusterimage {
	position:absolute;
	top:0px;
	left:320px;
	width:80px;
	height:80px;
	z-index:0;
}

.clustercontent {
	position:absolute;
	top:40px;
	left:40px;
	width:420px;
	height:220px;
	z-index:10;
	background-color:white;
	-moz-border-radius-topright: 10px;
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-bottomleft: 10px;
	-moz-border-radius-bottomright: 10px;

}

.clustercontent_inner {
	top:14px;
	left:14px;
	position:relative;
	width: 390px;
	height: 190px;
	border:1px solid black;
}

.cluster_letters {
	border-bottom:1px solid black;
	padding-left:5px;
	background-color:lightblue;
}

.cluster_letters a {
	color:navy;
	text-decoration:none;
}

.cluster_letters a:focus{
	color:grey;
	text-decoration:underline;
}

.cluster_list{
	overflow:auto;
	border-bottom:1px solid black;
	height:155px;
}

.cluster_pages {
	width:100%;
	margin-top:-2px;
}

.cluster_pages tr td{
	padding:0;
}

.cluster_contentList {
	padding-top:5px;
	width:100%;
	padding-bottom:15px;
	
}
