.frameBody
{
    font-family:sans-serif;
    font-size:12px;
    margin:0;
    width:100%;
    height:100%;
}

.frameBody p
{
    font-size:16px;
    padding-left:5px;
    text-align: justify;
    line-height: 1.2;
}

.rte-zone
{
	width:570px;
	margin:0;
	padding:0;
	height:330px;
    clear:both;
}

.rte-toolbar{ overflow:hidden; }

.rte-toolbar a, .rte-toolbar a img {
	border:0;
}

.rte-toolbar p {
    float:left;
    margin:0;
    padding-right:5px;
}

